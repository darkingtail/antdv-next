  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.3",
    "@types/node": "^24.7.1",
    "@types/stylis": "^4.0.2",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vue/test-utils": "^2.4.6",
    "vitest": "^3.2.4",
